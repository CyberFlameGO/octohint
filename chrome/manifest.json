{
  "manifest_version": 3,
  "name": "Octohint",
  "version": "2.0.8",
  "description": "IntelliSense hint for GitHub",
  "homepage_url": "https://github.com/pd4d10/octohint",
  "icons": {
    "128": "icons/logo.png"
  },
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://gist.github.com/*",
        "https://gitlab.com/*",
        "https://bitbucket.org/*"
      ],
      "js": ["dist/content-script.js"]
    }
  ],
  "options_ui": {
    "page": "dist/options.html"
  }
}
